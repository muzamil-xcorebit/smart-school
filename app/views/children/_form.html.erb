<%= form_with(model: [current_parent, child]) do |form| %>

  <div>
    <%= form.label :Name%> <br>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :Age%> <br>
    <%= form.text_field :age %>
  </div>
  <div>
    <%= form.label :Grades%><br>
    <%= form.select :grades, ApplicationRecord::Grade , { prompt: "Please select" } %>
  </div>
  <br>

  <%= render 'address/form' , form: form  %>
  <%= render 'pictures/form' , form: form  %>

<br>

  <div>
    <%= form.submit %>
  </div>
<% end %>
